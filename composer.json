{
  "version": 2,
  "builds": [
    {
      "src": "portfolio_managment_pro/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "portfolio_managment_pro/login.php"
    },
    {
      "src": "/dashboard",
      "dest": "portfolio_managment_pro/dashboard.php"
    },
    {
      "src": "/profile",
      "dest": "portfolio_managment_pro/profile.php"
    },
    {
      "src": "/(.*)",
      "dest": "portfolio_managment_pro/index.php"
    }
  ]
}
